<%- model_class = Note -%>
<div class="page-header">
    <h1>Issues</h1>
</div>

<% @all_notes.each do |note| %>
    <% if note.priority == "1" %>
    <% @class = "alert high"%>
    <% elsif note.priority == "2" %>
    <% @class = "alert medium"%>
    <% else %>
    <% @class = "alert low"%>
    <% end %>
    <div class="<%= @class %>">
        <a href="#"> <%= note.title %>
        </a>
        <div class="close" data-dismiss="alert">
            x
        </div>
    </div>
    <% end %>



